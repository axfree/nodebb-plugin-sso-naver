{
	"id": "nodebb-plugin-sso-naver",
	"name": "NodeBB Naver SSO",
	"description": "NodeBB Plugin that allows users to login/register via their Naver account.",
	"url": "https://github.com/axfree/nodebb-plugin-sso-naver",
	"library": "./library.js",
	"templates": "templates",
	"hooks": [
		{ "hook": "static:app.load", "method": "init" },
		{ "hook": "filter:config.get", "method": "exposeSettings" },
		{ "hook": "filter:auth.list", "method": "getAssociation" },
		{ "hook": "filter:auth.init", "method": "getStrategy" },
		{ "hook": "filter:admin.header.build", "method": "addMenuItem" },
		{ "hook": "static:user.delete", "method": "deleteUserData" },
		{ "hook": "filter:user.whitelistFields", "method": "appendUserHashWhitelist" }
	],
	"modules": {
		"../admin/plugins/sso-naver.js": "static/lib/admin.js"
	},
	"languages": "languages"
}
